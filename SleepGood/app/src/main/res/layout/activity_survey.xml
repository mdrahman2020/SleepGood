<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Survey">

    <CheckBox
        android:id="@+id/q2c1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginBottom="14dp"
        android:text="Nasal/Sinus allergies"
        app:layout_constraintBottom_toTopOf="@+id/q2c2"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q2c2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginBottom="15dp"
        android:text="Headache"
        app:layout_constraintBottom_toTopOf="@+id/q2c3"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q2c3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginBottom="15dp"
        android:text="Back Pain"
        app:layout_constraintBottom_toTopOf="@+id/q2c4"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q2c4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginLeft="68dp"
        android:layout_marginBottom="64dp"
        android:text="Chronic Pain"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/con1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="284dp"
        android:layout_marginLeft="284dp"
        android:layout_marginBottom="16dp"
        android:text="Continue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q1c1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="8dp"
        android:text="Coffee/Tea"
        app:layout_constraintBottom_toTopOf="@+id/q1c2"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q1c2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="24dp"
        android:text="Alcohol"
        app:layout_constraintBottom_toTopOf="@+id/q1c3"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q1c3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="20dp"
        android:text="Spicy Food"
        app:layout_constraintBottom_toTopOf="@+id/q1c4"
        app:layout_constraintStart_toStartOf="parent" />

    <CheckBox
        android:id="@+id/q1c4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginLeft="56dp"
        android:layout_marginBottom="52dp"
        android:text="Red Meat"
        app:layout_constraintBottom_toTopOf="@+id/textView4"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="337dp"
        android:layout_height="67dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginBottom="6dp"
        android:fontFamily="cursive"
        android:text="Did you eat/drink the following?"
        android:textColor="#1A8BC0"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/q1c1"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="337dp"
        android:layout_height="77dp"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginBottom="18dp"
        android:fontFamily="cursive"
        android:text="Are you suffering from any of the following?"
        android:textColor="#057BB1"
        app:autoSizeTextType="uniform"
        app:layout_constraintBottom_toTopOf="@+id/q2c1"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>